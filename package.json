{
  "name": "lang-feel",
  "version": "1.0.0",
  "description": "FEEL language support for the CodeMirror code editor",
  "scripts": {
    "all": "run-s lint build test",
    "test": "cm-runtests",
    "lint": "eslint . --ext ts",
    "build": "cm-buildhelper src/index.ts",
    "prepare": "npm run build",
    "dev": "chokidar 'src/*.ts' 'test/*.ts' --initial -c 'npm run build && npm test'"
  },
  "keywords": [
    "editor",
    "code"
  ],
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/nikku"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "sideEffects": false,
  "license": "MIT",
  "dependencies": {
    "@codemirror/autocomplete": "^6.9.1",
    "@codemirror/language": "^6.9.1",
    "@codemirror/state": "^6.2.1",
    "@codemirror/view": "^6.21.0",
    "@lezer/common": "^1.1.0",
    "lezer-feel": "^1.2.0"
  },
  "devDependencies": {
    "@codemirror/buildhelper": "^1.0.0",
    "@lezer/lr": "^1.3.12",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.50.0",
    "eslint-plugin-bpmn-io": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikku/lang-feel.git"
  },
  "files": [
    "dist"
  ]
}
